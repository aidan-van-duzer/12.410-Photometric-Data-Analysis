    "from scipy.stats import norm\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "03ae3456-3fdb-453c-ab8d-6fd172c68909",
   "metadata": {},
