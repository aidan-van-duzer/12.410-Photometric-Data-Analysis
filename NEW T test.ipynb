     "output_type": "stream",
     "text": [
      "P: 0.07361973925413613\n",
      "a: 0.9263802607458639\n",
      "Confidence limit = 7%\n"
     ]
    }
   ],
   "source": [
    "t = 0.0924\n",
    "x = np.linspace(-t, t, 100)\n",
    "y = norm.pdf(x)\n",
    "P = np.trapz(y, x=x)\n",
    "\n",
    "print('P:', P)\n",
    "print('a:', 1-P)\n",
    "print('Confidence limit = {:.0f}%'.format(P*100.0))"
   ]
